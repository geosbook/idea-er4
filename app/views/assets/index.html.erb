<h1>Listing assets</h1>

<table class="table table-bordered">
    <tr>
      <th>Name</th>
      <th>Type</th>
      <th>Lat</th>
      <th>Lng</th>
      <th>Speed</th>
      <th>Course</th>
      <th>Height</th>
      <th>Status</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>

    <% @assets.each do |asset| %>
      <tr>
        <td><%= asset.name %></td>
        <td><%= asset.type %></td>
        <td><%= asset.lat %></td>
        <td><%= asset.lng %></td>
        <td><%= asset.speed %></td>
        <td><%= asset.course %></td>
        <td><%= asset.height %></td>
        <td><%= asset.status %></td>
        <td><%= link_to 'Show', asset %></td>
        <td><%= link_to 'Edit', edit_asset_path(asset) %></td>
        <td><%= link_to 'Destroy', asset, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
</table>

<br>

<%= link_to 'New Asset', new_asset_path %>
